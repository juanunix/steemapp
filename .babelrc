{
  "presets": ["react-native"],
  "retainLines": true,  
  "plugins": [
    "transform-decorators-legacy",
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "components": "components",
          "search": "search",
          "home": "home",
          "auth": "auth",
          "user": "user",
          "organization": "organization",
          "discussion": "discussion",
          "issue": "issue",
          "api": "api",
          "config": "config",
          "utils": "utils",
          "locale": "locale",
          "package.json": "./package.json"
        }
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": ["ignite-ignore-reactotron"]
    }
  }
}
